<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Courgette|Pathway+Gothic+One|Palanquin+Dark|Kameron|Merriweather|PT+Sans|Poppins|Rancho|Ubuntu" rel="stylesheet">
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% include 'layout/header.html.twig' %}
        {% include 'layout/banner.html.twig' %}
        {% block body %}{% endblock %}
        {% include 'layout/footer.html.twig' %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
